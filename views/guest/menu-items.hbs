menu items
{{>guest-header}}
<div class="container mt-5 "> 
    <h2>All Items in {{category.name}}</h2>
    {{categoryName}}
    
   {{#each items}}
   <div class="card" style="width: 18rem;"> 
            <img src="/uploads/{{this.imageUrl}}" width="200" height="100" alt="Food Categories">  
            <div class="card-body">
                <p>Title</p>
                <h5 class="card-title">{{this.name}}</h5>
                <p class="card-text">{{this.description}}
                     
                </p>
                <a class="btn btn-primary" href="/">Add To Cart</a>
            </div>
        </div>
   {{/each}}
</div>