{{>manager-header}}
ongoing order page
<!-- onGoingOrders.hbs -->
<h1>Ongoing Orders</h1>

<table>
  <thead>
    <tr>
      <th>Item Name</th>
      <th>Quantity</th>
    </tr>
  </thead>
  <tbody>
    {{quantity}}
    {{#each items}}
      <tr>
        <td>{{this.name}}</td>
        <td>{{quantity}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>


